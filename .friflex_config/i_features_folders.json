{
    "folders": [
        "data",
        "data/repository",
        "data/models",
        "data/datasources",
        "domain",
        "domain/usecases",
        "domain/entities",
        "domain/repository",
        "presentation",
        "presentation/widgets",
        "presentation/screens",
        "presentation/blocs"
